<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Description</key>
    <string>Imports Keynote into Munki</string>
    <key>Identifier</key>
    <string>edu.goshen.sethku.keynote.munki</string>
    <key>Input</key>
    <dict>
        <key>MUNKI_REPO_SUBDIR</key>
        <string>Apple</string>
        <key>NAME</key>
        <string>Keynote</string>
        <key>pkginfo</key>
        <dict>
            <key>catalogs</key>
            <array>
                <string>testing</string>
            </array>
				<key>category</key>
				<string>Productivity</string>
				<key>developer</key>
				<string>Apple</string>
            <key>description</key>
            <string>Keynote is a presentation program from Apple</string>
            <key>display_name</key>
            <string>Keynote</string>
            <key>name</key>
            <string>%NAME%</string>
            <key>unattended_install</key>
            <true/>
        </dict>
    </dict>
    <key>MinimumVersion</key>
    <string>0.2.0</string>
    <key>ParentRecipe</key>
    <string>edu.goshen.sethku.keynote</string>
    <key>Process</key>
    <array>
        <dict>
            <key>Arguments</key>
            <dict>
                <key>pkg_path</key>
                <string>/tmp/Keynote.dmg</string>
					 <key>MUNKI_REPO</key>
					 <string>/Volumes/munki</string>
                <key>repo_subdirectory</key>
                <string>%MUNKI_REPO_SUBDIR%</string>
            </dict>
            <key>Processor</key>
            <string>MunkiImporter</string>
        </dict>
    </array>
</dict>
</plist>
