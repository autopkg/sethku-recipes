<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Comment</key>
	<string>Created with Recipe Robot v1.0.3 (https://github.com/homebysix/recipe-robot)</string>
	<key>Description</key>
	<string>Downloads the latest version of Evolus Pencil.</string>
	<key>Identifier</key>
	<string>com.github.sethku.download.EvolusPencil</string>
	<key>Input</key>
	<dict>
		<key>APP_FILENAME</key>
		<string>Pencil</string>
		<key>DOWNLOAD_URL</key>
		<string>http://pencil.evolus.vn/dl/Pencil-2.0.6-mac.tar.bz2</string>
		<key>NAME</key>
		<string>Evolus Pencil</string>
	</dict>
	<key>MinimumVersion</key>
	<string>0.6.1</string>
	<key>Process</key>
	<array>
		<dict>
			<key>Arguments</key>
			<dict>
				<key>re_pattern</key>
				<string>Pencil-([\d\.-]+).mac.tar.bz2</string>
				<key>result_output_var_name</key>
				<string>version</string>
				<key>url</key>
				<string>http://pencil.evolus.vn/Downloads.html</string>
			</dict>
			<key>Processor</key>
			<string>URLTextSearcher</string>
		</dict>
		<dict>
			<key>Arguments</key>
			<dict>
				<key>filename</key>
				<string>%NAME%.tgz</string>
				<key>url</key>
				<string>http://pencil.evolus.vn/dl/Pencil-%version%-mac.tar.bz2</string>
			</dict>
			<key>Processor</key>
			<string>URLDownloader</string>
		</dict>
		<dict>
			<key>Processor</key>
			<string>EndOfCheckPhase</string>
		</dict>
	</array>
</dict>
</plist>
